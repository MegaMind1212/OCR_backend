{
    "version": 2,
    "builds": [
      {
        "src": "api/index.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/transcribe",
        "methods": ["POST"],
        "dest": "api/index.py"
      },
      {
        "src": "/(.*)",
        "dest": "api/index.py"
      }
    ]
  }